<?xml version="1.0" encoding="UTF-8"?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="PagoEnLinea.Menu" Title="Menu">
   <MasterDetailPage.Master>
        <ContentPage Padding="20"
           Icon="hamburger.png" Title="Menu" >
            <StackLayout>
               <BoxView BackgroundColor = "#13445A" ></BoxView>
            </StackLayout>
            <ListView x:Name="ListMenu" ItemSelected="Handle_ItemSelected" SeparatorColor="Transparent" >
               <ListView.ItemTemplate>
                 <DataTemplate>
                      <ImageCell Text="{Binding MenuTitle}" Detail="{Binding MenuDetail}"></ImageCell>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
            </ContentPage>
    </MasterDetailPage.Master>
    <ContentPage.Content>
	</ContentPage.Content>
</MasterDetailPage>
